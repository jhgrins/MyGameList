{
    "name": "quaesta",
    "version": "1.0.0",
    "description": "A backlog for all the games you'll never play",
    "repository": "https://github.com/jhgrins/Quaesta",
    "license": "MIT",
    "scripts": {
        "types": "tsc",
        "lint": "eslint **/*.ts **/*.tsx",
        "check": "npm run types && npm run lint",
        "clean": "rm -rf .build .serverless coverage dist",
        "startFrontend": "vite",
        "startLambda": "serverless offline start",
        "start": "npm run clean && concurrently --kill-others-on-fail \"npm:startFrontend\" \"npm:startLambda\"",
        "buildFrontend": "vite build",
        "deployLambda": "serverless deploy --stage production",
        "unitTests": "TS_NODE_PROJECT='test/tsconfig.commonjs.json' mocha --recursive -r global-jsdom/register -r ts-node/register test/hooks.js test",
        "test": "c8 --all --check-coverage --lines 10 --include 'frontend' --include 'lambda' npm run unitTests"
    },
    "dependencies": {
        "@apollo/client": "^3.6.8",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "^5.0.0-alpha.86",
        "@mui/material": "^5.8.4",
        "@mui/styles": "^5.8.4",
        "apicalypse": "^0.1.9",
        "apollo-server-lambda": "^3.9.0",
        "apple-signin-auth": "^1.5.1",
        "autosuggest-highlight": "^3.2.1",
        "aws-sdk": "^2.1157.0",
        "axios": "^0.27.2",
        "casual": "^1.6.2",
        "crypto-js": "^4.1.1",
        "debounce": "^1.2.1",
        "dotenv": "^16.0.1",
        "framer-motion": "^6.3.11",
        "google-auth-library": "^8.0.2",
        "graphql": "^16.5.0",
        "graphql-ws": "^5.9.0",
        "handlebars": "^4.7.7",
        "jsonwebtoken": "^8.5.1",
        "react": "^18.2.0",
        "react-apple-login": "^1.1.3",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "recharts": "^2.1.13",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@testing-library/react": "^13.3.0",
        "@types/apicalypse": "^0.1.1",
        "@types/autosuggest-highlight": "^3.2.0",
        "@types/crypto-js": "^4.1.1",
        "@types/debounce": "^1.2.1",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.7.13",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
        "c8": "^7.11.3",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^7.2.2",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.1",
        "eslint-plugin-react": "^7.31.1",
        "global-jsdom": "^8.5.0",
        "jsdom": "^20.0.0",
        "mocha": "^10.0.0",
        "sass": "^1.54.5",
        "serverless": "^3.22.0",
        "serverless-dotenv-plugin": "^4.0.2",
        "serverless-dynamodb-local": "^0.2.40",
        "serverless-offline": "^9.3.0",
        "serverless-offline-ses": "^0.0.7",
        "serverless-offline-sns": "^0.76.0",
        "serverless-plugin-typescript": "^2.1.2",
        "serverless-prune-plugin": "^2.0.1",
        "serverless-static": "^1.0.0",
        "sinon": "^14.0.0",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4"
    }
}
