{
    "name": "quaesta",
    "version": "1.0.0",
    "description": "A backlog for all the games you'll never play",
    "repository": "https://github.com/jhgrins/Quaesta",
    "license": "MIT",
    "scripts": {
        "types": "tsc",
        "lint": "eslint **/*.ts **/*.tsx",
        "check": "npm run types && npm run lint",
        "clean": "rm -rf .build .serverless coverage dist",
        "startFrontend": "vite",
        "startLambda": "serverless offline start",
        "start": "npm run clean && concurrently --kill-others-on-fail \"npm:startFrontend\" \"npm:startLambda\"",
        "buildFrontend": "vite build",
        "deployLambda": "serverless deploy --stage production",
        "unitTests": "TS_NODE_PROJECT='test/tsconfig.commonjs.json' mocha --recursive -r global-jsdom/register -r ts-node/register test/hooks.js test",
        "test": "c8 --temp-directory .build/coverage --all --check-coverage --lines 10 --include 'frontend' --include 'lambda' npm run unitTests"
    },
    "dependencies": {
        "@apollo/client": "^3.7.2",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.11.0",
        "@mui/lab": "^5.0.0-alpha.112",
        "@mui/material": "^5.11.0",
        "apicalypse": "^0.2.0",
        "apollo-server-lambda": "^3.10.2",
        "apple-signin-auth": "^1.7.4",
        "autosuggest-highlight": "^3.3.4",
        "aws-sdk": "^2.1277.0",
        "axios": "^1.2.1",
        "casual": "^1.6.2",
        "crypto-js": "^4.1.1",
        "debounce": "^1.2.1",
        "dotenv": "^16.0.3",
        "framer-motion": "^7.10.2",
        "google-auth-library": "^8.7.0",
        "graphql": "^16.6.0",
        "graphql-ws": "^5.11.2",
        "jsonwebtoken": "^8.5.1",
        "react": "^18.2.0",
        "react-apple-login": "^1.1.6",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.5.0",
        "recharts": "^2.2.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@testing-library/react": "^13.4.0",
        "@types/apicalypse": "^0.1.1",
        "@types/autosuggest-highlight": "^3.2.0",
        "@types/crypto-js": "^4.1.1",
        "@types/debounce": "^1.2.1",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.11.17",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "@vitejs/plugin-react": "^3.0.0",
        "c8": "^7.12.0",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^7.6.0",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.11",
        "global-jsdom": "^8.6.0",
        "jsdom": "^20.0.3",
        "mocha": "^10.2.0",
        "sass": "^1.57.0",
        "serverless": "^3.25.1",
        "serverless-dotenv-plugin": "^4.0.2",
        "serverless-dynamodb-local": "^0.2.40",
        "serverless-offline": "^12.0.3",
        "serverless-offline-ses": "^0.0.7",
        "serverless-offline-sns": "^0.76.0",
        "serverless-plugin-typescript": "^2.1.4",
        "serverless-prune-plugin": "^2.0.1",
        "serverless-static": "^1.0.0",
        "sinon": "^15.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.2"
    }
}
